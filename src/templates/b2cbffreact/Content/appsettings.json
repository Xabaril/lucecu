{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureB2C": {
    "Instance": "[B2C instance, some like https://b2cname.b2clogin.com]",
    "Domain": "[B2C instance, some like b2cname.onmicrosoft.com] ",
    "ClientId": "[Client Id]",
    "ResponseType": "code",
    "SignUpSignInPolicyId": "[B2C Signup policy name, some like B2C_1_WIDSignin]",
    "SignedOutCallbackPath ": "[B2C Signup callback path, some like  /signout/B2C_1_WIDSignin]",
    "ClientSecret": "[Client Secret]",
    "UsePkce": "true"
  }
}
